{
  "name": "webbesoft/doorman",
  "description": "Privacy-first simple analytics for Laravel applications",
  "keywords": [
    "laravel",
    "analytics",
    "privacy",
    "filament"
  ],
  "type": "library",
  "require": {
    "php": "8.4.12",
    "illuminate/support": "^12.28",
    "laravel/pint": "^1.25",
    "filament/widgets": "^4"
  },
  "require-dev": {
    "orchestra/testbench": "^10.6",
    "phpunit/phpunit": "^12.3",
    "phpstan/phpstan": "^2.1",
    "pestphp/pest-plugin-laravel": "^4.0",
    "pestphp/pest": "^4.1"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Webbesoft\\Doorman\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Tawanda",
      "email": "ejmunongo@gmail.com"
    }
  ],
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "suggest": {
    "filament/filament": "Required for dashboard widgets"
  },
  "autoload-dev": {
    "psr-4": {
      "Webbesoft\\Doorman\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Webbesoft\\Doorman\\AnalyticsServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
